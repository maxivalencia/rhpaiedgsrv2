{% extends 'base.html.twig' %}

{% block title %}Nominations Personnels{% endblock %}

{% block body %}
    <h1>Nominations Personnels</h1>

    <table class="table">
        <tbody>
            {# <tr>
                <th>Id</th>
                <td>{{ nominations_personnel.id }}</td>
            </tr> #}
            <tr>
                <th>Date de nomination</th>
                <td>{{ nominations_personnel.dateNomination ? nominations_personnel.dateNomination|date('d/m/Y') : '' }}</td>
            </tr>
            <tr>
                <th>Rang</th>
                <td>{{ nominations_personnel.rang }}</td>
            </tr>
            <tr>
                <th>Echélon</th>
                <td>{{ nominations_personnel.echelon }}</td>
            </tr>
            <tr>
                <th>Class</th>
                <td>{{ nominations_personnel.class }}</td>
            </tr>
            <tr>
                <th>Indice</th>
                <td>{{ nominations_personnel.indice }}</td>
            </tr>
            <tr>
                <th>Grade</th>
                <td>{{ nominations_personnel.grade }}</td>
            </tr>
            <tr>
                <th>Personnel</th>
                <td>{{ nominations_personnel.personnel }}</td>
            </tr>
            <tr>
                <th>Décision</th>
                <td>{{ nominations_personnel.decision }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('nominations_personnels_index') }}" class="btn btn-primary"><i class="fa fa-arrow-left" aria-hidden="true"></i> Retour</a>
    <br>
    <a href="{{ path('nominations_personnels_edit', {'id': nominations_personnel.id}) }}" class="btn btn-success"><i class="fa fa-book" aria-hidden="true"></i> Modifier</a>

    {{ include('nominations_personnels/_delete_form.html.twig') }}
{% endblock %}
