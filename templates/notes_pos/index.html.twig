{% extends 'base.html.twig' %}

{% block title %}NotesPOS index{% endblock %}

{% block body %}
    <h1>NotesPOS index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Annee</th>
                <th>Qf</th>
                <th>Vet</th>
                <th>Ags</th>
                <th>Niveau</th>
                <th>Potentiel</th>
                <th>Appreciation_complet</th>
                <th>Personnel</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for notes_p_o in notes_p_o_ss %}
            <tr>
                <td>{{ notes_p_o.id }}</td>
                <td>{{ notes_p_o.annee ? notes_p_o.annee|date('Y-m-d') : '' }}</td>
                <td>{{ notes_p_o.qf }}</td>
                <td>{{ notes_p_o.vet }}</td>
                <td>{{ notes_p_o.ags }}</td>
                <td>{{ notes_p_o.niveau }}</td>
                <td>{{ notes_p_o.potentiel }}</td>
                <td>{{ notes_p_o.appreciationComplet }}</td>
                <td>{{ notes_p_o.personnel }}</td>
                <td>
                    <a href="{{ path('notes_p_o_s_show', {'id': notes_p_o.id}) }}">show</a>
                    <a href="{{ path('notes_p_o_s_edit', {'id': notes_p_o.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="10">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('notes_p_o_s_new') }}">Create new</a>
{% endblock %}
