{% extends 'base.html.twig' %}

{% block title %}Liste Notes POS{% endblock %}

{% block body %}
    <h1>Liste Notes POS</h1>

    <table class="table">
        <thead>
            <tr>
                {# <th>Id</th> #}
                <th>Année</th>
                {# <th>Qf</th> #}
                {# <th>Vet</th> #}
                {# <th>Ags</th> #}
                {# <th>Niveau</th> #}
                <th>Potentiel</th>
                <th>Appréciation complet</th>
                <th>Personnel</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for notes_p_o in notes_p_o_ss %}
            <tr>
                {# <td>{{ notes_p_o.id }}</td> #}
                <td>{{ notes_p_o.annee ? notes_p_o.annee|date('Y') : '' }}</td>
                {# <td>{{ notes_p_o.qf }}</td> #}
                {# <td>{{ notes_p_o.vet }}</td> #}
                {# <td>{{ notes_p_o.ags }}</td> #}
                {# <td>{{ notes_p_o.niveau }}</td> #}
                <td>{{ notes_p_o.potentiel }}</td>
                <td>{{ notes_p_o.appreciationComplet }}</td>
                <td>{{ notes_p_o.personnels }}</td>
                <td>
                    <a href="{{ path('notes_p_o_s_show', {'id': notes_p_o.id}) }}" class="btn btn-success"><i class="fa fa-book" aria-hidden="true"></i></a>
                    <a href="{{ path('notes_p_o_s_edit', {'id': notes_p_o.id}) }}" class="btn btn-danger"><i class="fa fa-trash" aria-hidden="true"></i></a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="10">Aucun enregistrement trouvé</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('notes_p_o_s_new') }}" class="btn btn-primary"><i class="fa fa-plus" aria-hidden="true"></i> Créer</a>
{% endblock %}
